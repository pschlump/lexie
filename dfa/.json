{
 "LexieData": {
  "TokenNames": [ "Tok_null", "Tok_CARROT", "Tok_CL", "Tok_COLON", "Tok_COMMA", "Tok_DOT", "Tok_EOF", "Tok_EQ", "Tok_EXCLAM", "Tok_GE", "Tok_GT"
   , "Tok_HTML", "Tok_ID", "Tok_LE", "Tok_LT", "Tok_L_AND", "Tok_L_EQ", "Tok_L_OR", "Tok_MINUS", "Tok_NE", "Tok_NE_LG", "Tok_NUM"
   , "Tok_OP", "Tok_OP_Tag", "Tok_OP_Var", "Tok_CL_Tag", "Tok_CL_Var", "Tok_OR", "Tok_PCT", "Tok_PLUS", "Tok_SLASH", "Tok_STAR"
   , "Tok_Str", "Tok_UE_Qt", "Tok_in", "Tok_and", "Tok_or", "Tok_not", "Tok_true", "Tok_false", "Tok_as", "Tok_export", "Tok_ID" ]
  , "StateNames": [  "S_Init", "S_TAG", "S_Com", "S_Esc", "S_Str0", "S_Str1", "S_Str2", "S_VAR", "S_Quote" ]
  , "MatchNames": [ "M_EOF" ]
  , "PrecondNames": [ "IsCommentEnabled" ]
  , "ErrorNames": [ "Warn_End_Var_Unexpected", "Err_EOF_Tag", "Err_EOF_Variable", "Err_EOF_In_String", "Err_EOF_Uneval_Quote" ]
 }
 , "StateSpec": [



 { "M": "\\{\\{",  "S":"S_Init",    "Ns":"S_VAR", "A":"A_Push",  "Rv":"Tok_OP_Var"                 }
,{ "M": "\\{%",   "S":"S_Init",    "Ns":"S_TAG", "A":"A_Push",  "Rv":"Tok_OP_Tag"                 }
,{ "M": "\\{\\%",  "S":"S_Init",       "A":"A_Repl",        "Replace":"{%"           }
,{ "M": "\\{\\\\\\{",  "S":"S_Init",       "A":"A_Repl",        "Replace":"{{"           }

,{ "M": ".*",   "S":"S_Init",       "A":"A_Greedy", "Rv":"Tok_HTML"                 }
,{ "MM": "M_EOF",  "S":"S_Init",       "A":"A_EOF",  "Rv":"Tok_EOF"                 }

,{ "M": "[a-zA-Z_][a-zA-Z0-9_]*", "S":"S_VAR|S_TAG",      "Rv":"Tok_ID"                  }
,{ "M": "[0-9]+",  "S":"S_VAR|S_TAG",         "Rv":"Tok_NUM"                 }
,{ "M": "\"",   "S":"S_VAR|S_TAG",  "Ns":"S_Str0", "A":"A_Push"                      }
,{ "M": "'",    "S":"S_VAR|S_TAG",  "Ns":"S_Str1", "A":"A_Push"                      }
,{ "M": "`",    "S":"S_VAR|S_TAG",  "Ns":"S_Str2", "A":"A_Push"                      }







,{ "M": "<=",   "S":"S_VAR|S_TAG",        "Rv":"Tok_LE"                   }
,{ "M": "\\^",   "S":"S_VAR|S_TAG",        "Rv":"Tok_CARROT"                  }
,{ "M": "==",   "S":"S_VAR|S_TAG",          "Rv":"Tok_L_EQ"                    }
,{ "M": ">=",   "S":"S_VAR|S_TAG",          "Rv":"Tok_GE"                    }
,{ "M": "&&",   "S":"S_VAR|S_TAG",          "Rv":"Tok_L_AND"                   }
,{ "M": "\\|\\|",  "S":"S_VAR|S_TAG",          "Rv":"Tok_L_OR"                    }
,{ "M": "!=",   "S":"S_VAR|S_TAG",          "Rv":"Tok_NE"                    }
,{ "M": "<>",   "S":"S_VAR|S_TAG",          "Rv":"Tok_NE_LG"                   }
,{ "M": "(",    "S":"S_VAR|S_TAG",          "Rv":"Tok_OP"                    }
,{ "M": ")",    "S":"S_VAR|S_TAG",          "Rv":"Tok_CL"                    }
,{ "M": "+",    "S":"S_VAR|S_TAG",          "Rv":"Tok_PLUS"                    }
,{ "M": "-",    "S":"S_VAR|S_TAG",          "Rv":"Tok_MINUS"                   }
,{ "M": "*",    "S":"S_VAR|S_TAG",          "Rv":"Tok_STAR"                    }
,{ "M": "<",    "S":"S_VAR|S_TAG",          "Rv":"Tok_LT"                    }
,{ "M": ">",    "S":"S_VAR|S_TAG",          "Rv":"Tok_GT"                    }
,{ "M": "/",    "S":"S_VAR|S_TAG",          "Rv":"Tok_SLASH"                   }
,{ "M": "\\,",   "S":"S_VAR|S_TAG",          "Rv":"Tok_COMMA"                   }
,{ "M": "\\.",   "S":"S_VAR|S_TAG",          "Rv":"Tok_DOT"                    }
,{ "M": "|",    "S":"S_VAR|S_TAG",          "Rv":"Tok_OR"                    }
,{ "M": "=",    "S":"S_VAR|S_TAG",          "Rv":"Tok_EQ"                    }
,{ "M": "%",    "S":"S_VAR|S_TAG",          "Rv":"Tok_PCT"                    }
,{ "M": " ",    "S":"S_VAR|S_TAG",          "Rv":"Tok_PCT"                    }







,{ "M": "!",    "S":"S_TAG",            "Rv":"Tok_EXCLAM"                   }
,{ "M": "%\\}",   "S":"S_TAG",    "Rv":"Tok_CL_TAG", "A":"A_Pop"                      }
,{ "M": "\\}\\}",  "S":"S_TAG",                 "Warn":"Warn_End_Var_Unexpected"       }
,{ "MM": "M_EOF",  "S":"S_TAG",    "A":"A_EOF",    "Rv":"Tok_EOF",     "Err":"Err_EOF_Tag"         }

,{ "M": ":",    "S":"S_VAR",            "Rv":"Tok_COLON"                   }
,{ "M": "\\}\\}",  "S":"S_VAR",    "Rv":"Tok_CL_VAR", "A":"A_Pop"                      }
,{ "M": "%\\}",   "S":"S_VAR",                 "Warn":"Warn_End_Tag_Unexpected"       }
,{ "MM": "M_EOF",  "S":"S_VAR",    "A":"A_EOF",    "Rv":"Tok_EOF",     "Err":"Err_EOF_Variable"        }


,{ "M": "\"",   "S":"S_Str0",    "A":"A_Pop",    "Rv":"Tok_Str",     "PostProc":["PostString"]        }
,{ "M": "\\",   "S":"S_Str0",    "Ns":"S_Esc", "A":"A_Push"                       }
,{ "M": ".*",   "S":"S_Str0", "A":"S_Greddy", "Ns":"S_Str0"                         }
,{ "MM": "M_EOF",  "S":"S_Str0",    "Ns":"S_Str0",   "Rv":"Tok_EOF",     "Err":"Err_EOF_In_String"        }

,{ "M": ".",    "S":"S_Esc",    "A":"A_Pop"                         }
,{ "MM": "M_EOF",  "S":"S_Esc",    "A":"A_Pop",    "Rv":"Tok_EOF",     "Err":"Err_EOF_In_String"        }


,{ "M": "'",    "S":"S_Str1",    "A":"A_Pop",    "Rv":"Tok_Str"                  }
,{ "M": ".*",   "S":"S_Str1", "A":"S_Greddy", "Ns":"S_Str1"                         }
,{ "M": "\\",   "S":"S_Str1",    "Ns":"S_Esc", "A":"A_Push"                       }
,{ "MM": "M_EOF",  "S":"S_Str1",    "Ns":"S_Str1",   "Rv":"Tok_EOF",     "Err":"Err_EOF_In_String"        }


,{ "M": "`",    "S":"S_Str2",    "A":"A_Pop",    "Rv":"Tok_Str"                  }
,{ "M": "\\",   "S":"S_Str1",    "Ns":"S_Esc", "A":"A_Push"                       }
,{ "M": ".*",   "S":"S_Str2", "A":"S_Greddy", "Ns":"S_Str2"                         }
,{ "MM": "M_EOF",  "S":"S_Str2",    "Ns":"S_Str2",   "Rv":"Tok_EOF",     "Err":"Err_EOF_In_String"        }


,{ "M": "#}",   "S":"S_Com",    "A":"A_Pop"                         }
,{ "M": "{\\#",   "S":"S_Com",    "A":"A_Repl",          "Replace":"{%"           }
,{ "M": "#\\}",   "S":"S_Com",    "A":"A_Repl",          "Replace":"#}"           }
,{ "MM": "M_EOF",  "S":"S_Com",    "A":"A_EOF",    "Rv":"Tok_EOF",     "Err":"Err_EOF_In_Comment"       }


,{ "M": "]]]",   "S":"S_Quote",   "A":"A_Pop",    "Rv":"Tok_UE_Qt"                  }
,{ "M": "[[[",   "S":"S_Quote",   "Ns":"S_Quote", "A":"A_Push"                       }
,{ "M": "[\\[[",   "S":"S_Quote",   "A":"A_Repl",          "Replace":"[[["           }
,{ "M": "[[\\[",   "S":"S_Quote",   "A":"A_Repl",          "Replace":"[[["           }
,{ "M": "]\\]]",   "S":"S_Quote",   "A":"A_Repl",          "Replace":"]]]"           }
,{ "M": "]]\\]",   "S":"S_Quote",   "A":"A_Repl",          "Replace":"]]]"           }
,{ "MM": "M_EOF",  "S":"S_Quote",   "A":"A_EOF",    "Rv":"Tok_EOF",     "Err":"Err_EOF_Uneval_Quote"       }

 ]
}

